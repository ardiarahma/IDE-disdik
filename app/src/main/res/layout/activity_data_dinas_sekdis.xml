<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DataDinasSekdisActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/blue_corner_bottom">

            <ImageView
                android:id="@+id/btn_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:clickable="true"
                android:src="@drawable/ic_arrow_back" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_5sdp"
                android:fontFamily="sans-serif-condensed-medium"
                android:gravity="center"
                android:text="Sekretariat"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="@dimen/_15ssp" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tvJabatan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:fontFamily="sans-serif-condensed-medium"
                android:text="..."
                android:textSize="@dimen/_15ssp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_73sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:background="@drawable/melon_1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvNamaStaf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="..."
                    android:textSize="@dimen/_14ssp" />

                <TextView
                    android:id="@+id/tvNipStaf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="..."
                    android:textSize="@dimen/_10ssp" />

                <TextView
                    android:id="@+id/tvPangkatStaf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="...."
                    android:textSize="@dimen/_10ssp" />

            </LinearLayout>

            <TextView
                android:id="@+id/tvJabatan2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:fontFamily="sans-serif-condensed-medium"
                android:text="..."
                android:textSize="@dimen/_15ssp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_73sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:background="@drawable/melon_2"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvNamaStaf2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="..."
                    android:textSize="@dimen/_14ssp" />

                <TextView
                    android:id="@+id/tvNipStaf2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="..."
                    android:textSize="@dimen/_10ssp" />

                <TextView
                    android:id="@+id/tvPangkatStaf2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="...."
                    android:textSize="@dimen/_10ssp" />

            </LinearLayout>

            <TextView
                android:id="@+id/tvJabatan3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:fontFamily="sans-serif-condensed-medium"
                android:text="..."
                android:textSize="@dimen/_15ssp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_73sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:background="@drawable/melon_3"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvNamaStaf3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="..."
                    android:textSize="@dimen/_14ssp" />

                <TextView
                    android:id="@+id/tvNipStaf3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="..."
                    android:textSize="@dimen/_10ssp" />

                <TextView
                    android:id="@+id/tvPangkatStaf3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:paddingStart="@dimen/_15sdp"
                    android:text="...."
                    android:textSize="@dimen/_10ssp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_2sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:background="@drawable/gradient_1" />

        </LinearLayout>

    </LinearLayout>


</androidx.core.widget.NestedScrollView>